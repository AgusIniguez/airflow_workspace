{
    "name": "Python 3.11 with Docker",
    "dockerFile": "Dockerfile",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter"
            ],
            "settings": {
                "python.pythonPath": "/usr/local/bin/python3"
            }
        }
    },      
    "hostRequirements": {
        "cpus": 8,
        "memory": "8gb",
        "storage": "32gb"
    }
}


  